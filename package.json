{
  "name": "hyperambient",
  "version": "0.3.0",
  "author": "@jamox",
  "repository": {
    "type": "git",
    "url": "git://github.com/jamox/hyperambient.git"
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/jamox/hyperambient/issues"
  },
  "scripts": {
    "test": "eslint index.js",
    "preinstall": "node preinstall.js"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "keywords": [
    "hyperterm",
    "ambient light"
  ],
  "description": "A Hyperterm theme that reacts to ambient light",
  "dependencies": {
    "ambientlight": "^0.1.2",
    "hyperterm-solarized-dark": "^0.1.6",
    "hyperterm-solarized-light": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "yoda": 0,
      "semi": [
        2,
        "never"
      ],
      "no-undef": 0,
      "no-unused-vars": 2,
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    }
  }
}
